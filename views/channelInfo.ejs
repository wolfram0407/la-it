<div class="InfoMainBox">
    <div class="channelInfoContainer">
        <div class="nickNameBox">
            <div class="channelInfoImg">
                <img src="<%= channel.channelImage %>" />
            </div>
            <div class="infoContentWrap">
                <div class="myChannelNickname"><%= channel.channelName %></div>
                <div class="infoEtc"><span class="spacelLeft"><%= channel.description %> </span></div>
            </div>
        </div>

        <div class="myChannelInfoDetail">
            <div hidden class="chargeBtn"><button>💜충전</button></div>
            <div class="blank"></div>

            <div class="startLiveBtn"><button id="goToStudioBtn">방송하기</button></div>
            <div class="channelSettingBtn spacelLeft btn" id="channelManagement">채널관리</div>
        </div>
    </div>

    <div class="channelInfoSettingContainer" hidden>
        <div class="channelInfoSettingWrap">
            <form class="channelInfoSettingForm" id="channelSettingForm" enctype="multipart/form-data">
                <h2 class="channelInfoTitle">채널 정보</h2>
                <div class="channelInfoSettingInputBoxName">
                    <span class="channelTitleHeader">채널명</span>
                    <input
                        type="text"
                        name="channelName"
                        class="channelTitleInput"
                        maxlength="10"
                        aria-label="Sizing example input"
                        aria-describedby="inputGroup-sizing-lg"
                        value="<%=  channel.channelName %>"
                    />
                </div>
                <div class="channelImageWrap" id="channelInfoSettingImage">
                    <span class="channelTitleHeader">썸네일</span>
                    <div class="channelImageINputWrap">
                        <input type="file" name="file" accept="image/jpg, image/jpeg" class="fileUploadForm" id="inputGroupFile04" aria-describedby="inputGroupFileAddon04" aria-label="Upload" />
                        <button class="uploadBtn" type="button">Upload</button>
                    </div>
                </div>
                <div class="settingStreamkeyInputWrap">
                    <span class="channelTitleKeyHeader" id="inputGroup-sizing-default-streamKey">스트림키</span>
                    <div class="getNewKeyWrap">
                        <span id="streamKeyValue" class="streamkeyVal" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-lg"><%= channel.streamKey %></span>
                        <button class="getNewStreamkeyBtn" type="button" id="streamKeyGetNew">재발급</button>
                    </div>
                </div>
                <div class="channelInfoSettingInput" id="channelInfoSettingTextWrap">
                    <span class="channelTitleHeader">상세정보</span>
                    <textarea name="channelDescription" class="channelInfoText" id="channelInfoSettingText" maxlength="50" placeholder="채널 소개글을 작성해 주세요"><%=channel.description%></textarea>
                </div>
                <div class="channelInfoBtnWrap">
                    <button type="submit" class="channelSettingSaveBtn">저장</button>
                    <button type="button" id="exitBtn" class="channelSettingSaveBtn">닫기</button>
                </div>
            </form>
        </div>
    </div>

    <div class="homeBox" hidden>
        <!-- 비디오 박스 시작-->
